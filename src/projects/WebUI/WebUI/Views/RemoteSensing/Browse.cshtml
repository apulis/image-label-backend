@{
    ViewData["Title"] = "Browse";
}

<div class="container body-content" ng-app="fileUpload" ng-controller="MyCtrl">

    <div class="modal-content">

        <br />
        <br />
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h2 class="modal-title" id="submitModalLabel">Browse Image</h2>
        </div>

        <partial name="_PrefixView" />


        <img ng-src="{{currentImage}}" ng-if="currentImage">
        <div ng-if="currentImage">
            <label>{{currentImage}}</label>
        </div>

        <partial name="_MoveAround" />

        <div ng-if="onemeta">
            <label>{{onemeta}}</label>
        </div>

        <div layout="row" ng-if="current.editable" >
            <div class="col-md-1">
                <button class="btn btn-default" id="mergeId">Merge</button>
                <button class="btn btn-default" id="sliceId">Slice</button>
                <button class="btn btn-default" id="undoId">Undo</button>
                <button class="btn btn-default" id="redoId">Redo</button>
                <button class="btn btn-default" id="saveId">Save</button>

            </div>
            
        </div>

    </div>


</div>

<div class="col-md-11">
    <canvas id="canvasOutput" width="1024" height="1024"></canvas>
</div>

<script src="~/lib/remote-sensing/remote-sensing.js"></script>